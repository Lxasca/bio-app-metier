{{ form_start(form) }}

<div class="d-flex">
	{{ form_row(form.title) }}
</div>

{{ form_row(form.scenario) }}

<div class="form-checkbox-group">
	{% for choice in form.equipments %}
		<div class="form-checkbox-item">
			{{ form_widget(choice) }}
			{{ form_label(choice) }}
		</div>
	{% endfor %}
</div>

<section class="d-flex justify-content-end">
	{{ form_row(form.submit) }}
	{% if isCreate is not defined  %}
		{{ include('test/_delete_form.html.twig') }}
	{% endif %}
</section>
{{ form_end(form) }}
