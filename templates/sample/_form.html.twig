{{ form_start(form) }}

<div class="d-flex">
	{{ form_row(form.status) }}

	<span class="mx-3">
		{{ form_row(form.collectionDate) }}
	</span>
</div>

<div class="col-4">
	{{ form_row(form.type) }}
</div>
<div class="col-4">
	{{ form_row(form.source) }}
</div>

<section class="d-flex justify-content-end">
	{{ form_row(form.submit) }}
	{% if isCreate is not defined  %}
		{{ include('sample/_delete_form.html.twig') }}
	{% endif %}
</section>

{{ form_end(form) }}
